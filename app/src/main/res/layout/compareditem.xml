<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical" >
    <TextView
        android:layout_width="@dimen/item_width" 
        android:layout_height="@dimen/item_height"
        android:textSize="@dimen/text_14"
        android:gravity="center"
        android:background="@drawable/item_compare_border"
         android:padding="5dp"
         android:text="实勘图"
        />
    <TextView 
        android:layout_width="@dimen/item_width" 
        android:layout_height="@dimen/item_height"
        android:textSize="@dimen/text_14"
        android:gravity="center"
        android:background="@drawable/item_compare_border"
         android:padding="5dp"
         android:text="户型图"
     />
    
    <TextView 
        android:layout_width="@dimen/item_width" 
        android:layout_height="wrap_content"
        android:textSize="@dimen/text_14"
        android:gravity="center"
        android:background="@drawable/item_compare_border"
         android:padding="5dp"
         android:text="价格:"
        />
    <TextView 
        android:layout_width="@dimen/item_width" 
        android:layout_height="wrap_content"
        android:textSize="@dimen/text_14"
        android:gravity="center"
        android:background="@drawable/item_compare_border"
         android:padding="5dp"
         android:text="楼盘名:"
        />
    <TextView 
        android:layout_width="@dimen/item_width" 
        android:layout_height="wrap_content"
        android:text="单价:"
        android:textSize="@dimen/text_14"
        android:gravity="center"
        android:background="@drawable/item_compare_border"
         android:padding="5dp"
        />
    <TextView 
        android:layout_width="@dimen/item_width" 
        android:layout_height="wrap_content"
        android:text="总成本:"
        android:textSize="@dimen/text_14"
        android:gravity="center"
        android:background="@drawable/item_compare_border"
         android:padding="5dp"
        />
    <TextView
        android:layout_width="@dimen/item_width" 
        android:layout_height="wrap_content"
        android:textSize="@dimen/text_14"
        android:text="首套可贷:"
        android:gravity="center"
        android:background="@drawable/item_compare_border"
         android:padding="5dp"
        />
    <TextView 
        android:layout_width="@dimen/item_width" 
        android:layout_height="wrap_content"
        android:text="二套可贷"
        android:textSize="@dimen/text_14"
        android:gravity="center"
        android:background="@drawable/item_compare_border"
         android:padding="5dp"
        />
    <TextView 
        android:layout_width="@dimen/item_width" 
        android:layout_height="wrap_content"
        android:text="居室:"
        android:textSize="@dimen/text_14"
        android:gravity="center"
        android:background="@drawable/item_compare_border"
         android:padding="5dp"
        />
    <TextView 
        android:layout_width="@dimen/item_width" 
        android:layout_height="wrap_content"
        android:text="面积:"
        android:textSize="@dimen/text_14"
        android:gravity="center"
        android:background="@drawable/item_compare_border"
         android:padding="5dp"
        />
    <TextView 
        android:layout_width="@dimen/item_width" 
        android:layout_height="wrap_content"
        android:text="楼层:"
        android:textSize="@dimen/text_14"
        android:gravity="center"
        android:background="@drawable/item_compare_border"
         android:padding="5dp"
        />
    <TextView 
        android:layout_width="@dimen/item_width" 
        android:layout_height="wrap_content"
        android:textSize="@dimen/text_14"
        android:text="朝向:"
        android:gravity="center"
        android:background="@drawable/item_compare_border"
         android:padding="5dp"
        />
    <TextView 
        android:layout_width="@dimen/item_width" 
        android:layout_height="wrap_content"
        android:text="建成年代:"
        android:textSize="@dimen/text_14"
        android:gravity="center"
        android:background="@drawable/item_compare_border"
        android:padding="5dp"
        />
    <TextView 
        android:layout_width="@dimen/item_width" 
        android:layout_height="wrap_content"
        android:text="住宅类型:"
        android:textSize="@dimen/text_14"
        android:gravity="center"
        android:background="@drawable/item_compare_border"
         android:padding="5dp"
        />
    <TextView 
        android:layout_width="@dimen/item_width" 
        android:layout_height="wrap_content"
        android:text="装修:"
        android:textSize="@dimen/text_14"
        android:gravity="center"
        android:background="@drawable/item_compare_border"
         android:padding="5dp"
        />
    <TextView 
        android:layout_width="@dimen/item_width" 
        android:layout_height="wrap_content"
        android:text="城区:"
        android:textSize="@dimen/text_14"
        android:gravity="center"
        android:background="@drawable/item_compare_border"
         android:padding="5dp"
        />
    <TextView 
        android:layout_width="@dimen/item_width" 
        android:layout_height="wrap_content"
        android:text="商圈:"
        android:textSize="@dimen/text_14"
        android:gravity="center"
        android:background="@drawable/item_compare_border"
         android:padding="5dp"
        />
    <TextView 
        android:layout_width="@dimen/item_width" 
        android:layout_height="wrap_content"
        android:text="地址:"
        android:textSize="@dimen/text_14"
        android:gravity="center"
        android:background="@drawable/item_compare_border"
         android:padding="5dp"
        />
    <RelativeLayout 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        >
        <TextView 
        	 android:layout_width="@dimen/item_width" 
       		 android:layout_height="@dimen/item_height"
       		  android:text="推荐顾问:"
       		 android:textSize="@dimen/text_14"
       	 	 android:gravity="center"
       	 	 android:id="@+id/tv_phone"
        	 android:background="@drawable/item_compare_border"
        	  android:padding="5dp"
       	 />
   		 <TextView 
   		     android:layout_below="@id/tv_phone"
        	 android:layout_width="@dimen/item_width" 
       		 android:layout_height="wrap_content"
       		  android:text="电话:"
       		 android:textSize="@dimen/text_14"
       	 	 android:gravity="center"
        	 android:background="@drawable/item_compare_border"
        	  android:padding="5dp"
       	 />
    </RelativeLayout>
    
    

</LinearLayout>
